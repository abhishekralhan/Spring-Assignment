	version="2.5">
	<welcome-file-list>
		<welcome-file>/WEB-INF/jsp/UserLogin</welcome-file>
	</welcome-file-list>
	<servlet>
		<servlet-name>SpringMVCDemo</servlet-name>
		<servlet-class>org.springframework.web.servlet.DispatcherServlet</servlet-class>
		<load-on-startup>1</load-on-startup>
